{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "intune-azure/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/media/**",
          "**/*.png",
          "**/*.gif",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "intune-azure/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/enterprise-mobility-security/toc.json",
      "product_feedback_displaytext": "Intune Feedback",
      "product_feedback_url": "https://microsoftintune.uservoice.com/",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/Intunedocs.zh-tw",
      "feedback_product_url": "https://microsoftintune.uservoice.com/",
      "searchScope": [
        "Intune"
      ],
      "disable_contribution_on_translation_quality": [
        "MT",
        "MTE75",
        "MTE95",
        "Blank"
      ]
    },
    "fileMetadata": {
      "featureFlags": {
        "*.md": [
          "show_binary_rating"
        ]
      }
    },
    "template": [],
    "dest": "intune-azure",
    "markdownEngineName": "markdig"
  }
}